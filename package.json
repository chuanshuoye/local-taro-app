{
  "name": "local-taro-app",
  "private": true,
  "version": "1.0.0",
  "description": "基于taro脚手架开发H5、小程序多端应用",
  "scripts": {
    "dev:weapp": "cd apps/myApp && taro build --type weapp --watch",
    "dev:h5": "cd apps/myApp && taro build --type h5 --watch",
    "build:weapp": "cd apps/myApp && taro build --type weapp",
    "build:h5": "cd apps/myApp && taro build --type h5"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "utils/*"
  ],
  "devDependencies": {
    "@babel/plugin-syntax-typescript": "^7.25.9"
  }
}